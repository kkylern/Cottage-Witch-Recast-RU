          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"Журнал §kЗАШИФРОВАНО\\\"},
                {\\\"text\\\":\\\"§l\n\n\\\"},
                {\\\"text\\\":\\\"Я следовал по следу магического ока, чтобы добраться до этой заброшенной, но удивительной крепости. \\\"},
                {\\\"text\\\":\\\"Тут давно никого нет, кроме орд нежити. \\\"},
                {\\\"text\\\":\\\"Я нашёл несколько странных древних журналов, которые предполагают, что это место\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"было домом древних строителей. \\\"},
                {\\\"text\\\":\\\"У них есть информация, которая затёрта зашифрованным языком. \\\"},
                {\\\"text\\\":\\\"Интересно, какая тёмная магия это вызвала.\\\"}
              ]\"
            ],
            title:Журнал,
            author:Неизвестный,
            display:{Lore:[\"Найдена в древней крепости\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "Журнал путешественника #1",
              "add": false
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 1,
