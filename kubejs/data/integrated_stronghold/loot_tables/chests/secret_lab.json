	{
	  "type": "minecraft:item",
	  "weight": 1,
	  "name": "minecraft:written_book",
	  "functions": [
		{
		  "function": "set_nbt",
		  "tag": "{
			pages:[
			  \"[
				\\\"\\\",
				{\\\"text\\\":\\\"Журнал §kЗАШИФРОВАНО\\\"},
				{\\\"text\\\":\\\"§l\nДень 318\n\n\\\"},
				{\\\"text\\\":\\\"Я заразился. На обратном пути, после деактивации и запирания портала, \\\"},
				{\\\"text\\\":\\\"я встретил тёмную фигуру, которая напоминала \\\"},
				{\\\"text\\\":\\\"§kЗАШИФРОВАНО.\\\"},
				{\\\"text\\\":\\\"Она подкрались сзади и застала меня врасплох.\\\"}
			  ]\",
			  \"[
				{\\\"text\\\":\\\"С тех пор меня тошнит, а вода обжигает как раскалённая лава. \\\"},
				{\\\"text\\\":\\\"Как странно. Моя кожа также покрывается тьмой. \\\"},
				{\\\"text\\\":\\\"Остались считанные часы, прежде чем я потеряю себя.\\\"}
			  ]\"
			],
			title:Журнал,
			author:Неизвестный,
			display:{Lore:[\"Найдена в древней крепости\"]}
		  }"
		},
		{
		  "function": "minecraft:set_name",
